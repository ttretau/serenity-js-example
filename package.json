{
  "name": "Serenity-JS-example",
  "version": "1.0.0",
  "description": "E2E Tests showing Serenity-JS",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf public target",
    "postinstall": "npm run webdriver:update -- --standalone",
    "prestart": "serenity update",
    "report": "serenity run",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "tslint": "tslint",
    "start": "failsafe clean e2etestk8s_browserstack report",
    "protractor": "protractor ./protractor.conf.js --params.seleniumAddress=",
    "e2etestk8s_browserstack": "protractor protractor.conf.js --params.threads=1 --params.seleniumAddress=http://hub-cloud.browserstack.com/wd/hub",
    "webdriver-manager": "webdriver-manager",
    "webdriver:update": "npm run webdriver-manager update"
  },
  "devDependencies": {
    "@types/chai": "3.4.35",
    "@types/chai-as-promised": "0.0.30",
    "@types/cucumber": "0.0.38",
    "@types/node": "7.0.8",
    "moment": "2.17.1",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-smoothie": "0.2.1",
    "cucumber": "1.3.2",
    "cucumber-html-report": "0.5.6",
    "npm-failsafe": "^0.2.1",
    "protractor": "5.1.1",
    "protractor-cucumber-framework": "1.0.2",
    "rimraf": "^2.5.4",
    "serenity-cli": "^0.2.3",
    "serenity-js": "^1.2.4",
    "ts-node": "2.1.0",
    "tslint": "4.5.1",
    "typescript": "2.2.1"
  }
}
